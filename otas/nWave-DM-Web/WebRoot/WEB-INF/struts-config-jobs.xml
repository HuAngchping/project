      attribute="JobsJobDiscoveryForm"
      input="tile.jobs.edit.discovery"
      name="JobsJobDiscoveryForm"
      scope="request"
      type="com.npower.dm.action.jobs.SaveJobDiscoveryAction"
      roles="authenticated">
      <forward
        name="edit"
