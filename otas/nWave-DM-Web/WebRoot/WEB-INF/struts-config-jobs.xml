      <forward
        name="schedule"
        path="tile.jobs.edit.discovery.schedule"
        contextRelative="true" />
      <forward
        name="success"
        path="tile.jobs.success.jobDiscovery"
        contextRelative="true" />
      <forward
        name="cancel"
        path="/jobs/SelectJobType.do"
        contextRelative="true"
        redirect="true"/>
    </action>
    
    <action
      path="/jobs/SaveScirpt"
      attribute="JobsJobScriptForm"
      name="JobsJobScriptForm"
      input="tile.jobs.edit.jobScript"
      parameter="action"
      scope="request"
      type="com.npower.dm.action.jobs.SaveJobScriptAction"
      validate="true"
      roles="authenticated">
      <forward
        name="schedule"
        path="tile.jobs.view.jobScript.schedule"
        contextRelative="true" />
