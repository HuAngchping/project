  <action-mappings >
    
    <!-- Job List: all of jobs -->
    <action
      path="/jobs/list/all"
      attribute="SearchJobForm"
      name="JobsSearchJobForm"
      scope="request"
      type="com.npower.dm.action.jobs.Jobs4AllAction"
      roles="authenticated">
      <forward
        name="list"
        path="tile.jobs.list.all"
        contextRelative="true" />
    </action>

    <!-- Job List: all of queued jobs -->
    <action
      path="/jobs/list/queued"
      attribute="SearchJobForm"
      name="JobsSearchJobForm"
      scope="request"
      type="com.npower.dm.action.jobs.Jobs4QueuedAction"
      roles="authenticated">
      <forward
        name="list"
        path="tile.jobs.list.queued"
        contextRelative="true" />
    </action>
    
    <!-- Job List: all of finished jobs -->
    <action
      path="/jobs/list/finished"
      attribute="SearchJobForm"
      name="JobsSearchJobForm"
      scope="request"
      type="com.npower.dm.action.jobs.Jobs4FinishedAction"
      roles="authenticated">
      <forward
        name="list"
        path="tile.jobs.list.finished"
        contextRelative="true" />
    </action>
    
    
    <action
      attribute="JobViewForm"
      name="JobsJobViewForm"
      path="/jobs/JobView"
      scope="request"
      type="com.npower.dm.action.jobs.JobViewAction"
      validate="false"
      roles="authenticated">
      <!-- DM Profile Assignment -->
      <forward
        name="view.dm.assign.profile"
        path="tile.jobs.job.view.dm.assign.profile"
        contextRelative="true" />
      <!-- DM Profile Re-Assignment -->
      <forward
        name="view.dm.re_assign.profile"
        path="tile.jobs.job.view.dm.re_assign.profile"
        contextRelative="true" />
      <!-- DM Device Discovery -->
      <forward
        name="view.dm.discovery"
        path="tile.jobs.job.view.dm.discovery"
        contextRelative="true" />
      <!-- DM Command script -->
      <forward
        name="view.dm.command.script"
        path="tile.jobs.job.view.dm.command.script"
        contextRelative="true" />
       <!-- DM Firmware Update -->
      <forward
        name="view.dm.firmware.update"
        path="tile.jobs.job.view.dm.firmware.update"
        contextRelative="true" />
       <!-- DM Software Install -->
      <forward
        name="view.dm.software.install"
        path="tile.jobs.job.view.dm.software.install"
        contextRelative="true" />
       <!-- DM Software UnInstall -->
      <forward
        name="view.dm.software.uninstall"
        path="tile.jobs.job.view.dm.software.uninstall"
        contextRelative="true" />
       <!-- DM Software Active -->
      <forward
        name="view.dm.software.active"
        path="tile.jobs.job.view.dm.software.active"
        contextRelative="true" />
       <!-- DM Software Deactive -->
      <forward
        name="view.dm.software.deactive"
        path="tile.jobs.job.view.dm.software.deactive"
        contextRelative="true" />
       <!-- DM Software Upgrade -->
      <forward
        name="view.dm.software.upgrade"
        path="tile.jobs.job.view.dm.software.upgrade"
        contextRelative="true" />
       <!-- DM Software Discovery -->
      <forward
        name="view.dm.software.discovery"
        path="tile.jobs.job.view.dm.software.discovery"
        contextRelative="true" />
      <!-- CP Profile Assignment -->
      <forward
        name="view.cp.assign.profile"
        path="tile.jobs.job.view.cp.assign.profile"
        contextRelative="true" />
    </action>
    
    <action
      path="/jobs/Job"
      parameter="action"
      attribute="JobsOperationForm"
      name="JobsOperationForm"
      scope="request"
      type="com.npower.dm.action.jobs.JobAction"
      validate="false"
      roles="authenticated">
      <forward
        name="view"
        path="/jobs/JobView.do"
        contextRelative="true" />
      <forward
        name="job.list.queued"
        path="/jobs/list/queued.do"
        contextRelative="true" />
      <forward
        name="job.list.finished"
        path="/jobs/list/finished.do"
        contextRelative="true" />
      <forward
        name="job.list.all"
        path="/jobs/list/all.do"
        contextRelative="true" />
    </action>

    <!-- Select Job Type -->
    <action
      path="/jobs/SelectJobType"
      parameter="method"
      name="JobsCreateJobForm"
      attribute="JobsCreateJobForm"
      scope="request"
      type="com.npower.dm.action.jobs.SelectJobTypeAction"
      validate="false"
      roles="authenticated">
      <forward
        name="SelectJobType"
        path="tile.jobs.edit.jobType"
        contextRelative="true" />
      <forward
        name="setTargetDevices"
        path="/jobs/SetTargetDevices.do"
        contextRelative="true" />
      <forward
        name="cancel"
        path="/jobs/list/queued.do"
        contextRelative="true"
        redirect="true" />
    </action>

    <!-- Set Target Devices -->
