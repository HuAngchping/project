        contextRelative="true" />
      <forward
        name="cancel"
        path="/jobs/SelectJobType.do"
        contextRelative="true"
        redirect="true" />
    </action>

    <action
      path="/jobs/EditJobType"
      parameter="jobType"
      name="JobsCreateJobForm"
      attribute="JobsCreateJobForm"
      input="/jobs/SetTargetDevices.do"
      scope="request"
      type="com.npower.dm.action.jobs.EditJobAction"
      validate="true"
      roles="authenticated">
      <forward
        name="cancel"
        path="/jobs/SelectJobType.do"
        contextRelative="true"
        redirect="true"/>
      <forward
        name="EditScript"
        path="tile.jobs.edit.jobScript"
        contextRelative="true" />
      <forward
        name="WizardAssignProfile4DM"
        path="/jobs/dm/SelectProfileConfig.do"
        contextRelative="true" />
      <forward
        name="WizardAssignProfile4CP"
        path="/jobs/cp/SelectProfileConfig.do"
        contextRelative="true" />
      <forward
        name="EditDiscoveryNode"
        path="/jobs/SaveDiscoveryJob.do"
        contextRelative="true" />
      <forward
        name="SelectJobType"
        path="/jobs/SelectJobType.do"
        contextRelative="true" />
      <forward
        name="software.install.select.software"
        path="/jobs/SelectSoftware4Install.do"
        contextRelative="true" />
      <forward
        name="software.discovery"
        path="/jobs/job/software/discovery/save.do"
        contextRelative="true" />
      <forward
        name="software.uninstall.select.software"
        path="tile.jobs.edit.software.uninstall.select.software"
        contextRelative="true" />
      <forward
        name="software.active.select.software"
        path="tile.jobs.edit.software.active.select.software"
        contextRelative="true" />
      <forward
        name="software.deactive.select.software"
        path="tile.jobs.edit.software.deactive.select.software"
        contextRelative="true" />
      <forward
        name="software.upgrade.select.software"
        path="tile.jobs.edit.software.upgrade.select.software"
        contextRelative="true" />
      <forward
        name="fota.select.update"
        path="tile.jobs.edit.fota.select.update"
        contextRelative="true" />
    </action>
    <action 
      path="/jobs/SelectSoftware4Install"
      type="com.npower.dm.action.jobs.SelectSoftware4InstallAction"
      name="JobsSelectSoftware4InstallForm"
      attribute="JobsSelectSoftware4InstallForm"
